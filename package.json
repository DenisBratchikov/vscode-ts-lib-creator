{
	"name": "create-lib-structure",
	"displayName": "Typescript library structure creator",
	"description": "Creates file structure for library on TypeScript",
	"version": "0.0.1",
	"license" : "(ISC OR GPL-3.0)",
	"publisher": "DenisBratchikov",
	"repository": {
		"type": "git",
		"url": "https://github.com/DenisBratchikov/vscode/tree/master/ts-lib-creator"
	},
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"keywords": ["typescript", "tensor", "library", "structure creator", "file structure"],
	"activationEvents": [
		"onCommand:extension.createLibrary"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.createLibrary",
				"title": "Create Library"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^10.14.17",
		"@types/vscode": "^1.34.0",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1"
	}
}
